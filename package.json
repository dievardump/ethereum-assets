{
  "name": "ethereum-assets",
  "version": "0.0.1",
  "description": "List of Ethereum Tokens (erc20, nft...)",
  "npmName": "ethereum-assets",
  "main": "dist/index.json",
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "prod": "cross-env NODE_ENV=production npm-run-all build-js",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel watch-js",
    "build": "node ./src/generate.js",
    "format": "prettier --write \"{src,data}/**/*.{js,json}\"",
    "prepare": "npm run clean && npm run format && npm run build"
  },
  "dependencies": {
    "filehound": "^1.16.2",
    "minimist": "^1.2.0",
    "node-filelist": "^1.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "glob": "^7.1.2",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  }
}
